#include <assert.h>
#include <limits.h>
#include <math.h>
#include <stdbool.h>
#include <stddef.h>
#include <stdint.h>
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main(){
   int i;
   int n;
   scanf("%d", &n);
   for(i=1;i<=10;i++){
     int x=n*i; 
     printf("%d x %d = %d\n",n,i,x);
    }
  
return 0;
}
